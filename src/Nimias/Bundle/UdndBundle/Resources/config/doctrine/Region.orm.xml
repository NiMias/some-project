<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="Nimias\Bundle\UdndBundle\Entity\Region" table="region">
    <indexes>
      <index name="district" columns="district"/>
      <index name="microd" columns="microdistrict"/>
      <index name="misto" columns="misto_id"/>
    </indexes>
    <id name="regionId" type="integer" column="region_id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="district" type="string" column="district" length="255" nullable="true"/>
    <field name="microdistrict" type="string" column="microdistrict" length="255" nullable="true"/>
    <field name="mistoId" type="integer" column="misto_id" nullable="false"/>
  </entity>
</doctrine-mapping>
